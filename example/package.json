{
  "name": "PeertalkerExample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "build:ios": "mkdirp dist && react-native bundle --entry-file index.tsx --platform ios --dev true --bundle-output dist/main.ios.jsbundle --assets-dest dist",
    "build:macos": "mkdirp dist && react-native bundle --entry-file index.tsx --platform macos --dev true --bundle-output dist/main.macos.jsbundle --assets-dest dist",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "macos": "react-native run-macos --scheme PeertalkerExample",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-macos": "^0.63.0",
    "react-native-peertalker": "../"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "mkdirp": "^1.0.0",
    "react-native-test-app": "^1.1.6",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
